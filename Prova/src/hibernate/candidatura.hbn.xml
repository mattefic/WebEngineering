<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="model.Candidatura" table="candidatura">
		<id name="idCandidatura" column="idCandidatura">
			<generator class="assigned"></generator>
		</id>
		<property name="idOfferta" column="idOfferta">
		</property>
		<property name="idTutore" column="idTutore"></property>
		<property name="idUtente" column="idUtente"></property>
		<property name="stato" column="stato"></property>
		<property name="dataCanditatura" column="dataCanditatura"></property>
	</class>
</hibernate-mapping> 