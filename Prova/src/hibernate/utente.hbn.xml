<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="model.Utente" table="utente">
		<id name="idUtente" column="idUtente">
		</id>
		<property name="codiceFiscale" column="codiceFiscale">
		</property>
		<property name="nome" column="nome"></property>
		<property name="cognome" column="cognome"></property>
		<property name="email" column="email"></property>
		<property name="luogoNascita" column="luogoNascita"></property>
		<property name="residenza" column="residenza"></property>
		<property name="corsoLaurea" column="corsoLaurea"></property>
		<property name="handicap" column="handicap"></property>
		<property name="telefono" column="telefono"></property>
		<property name="dataNascita" column="dataNascita"></property>
		<property name="password" column="password"></property>
		<property name="tipo" column="tipo"></property>
	</class>

</hibernate-mapping>  